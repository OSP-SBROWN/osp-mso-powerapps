                              - btn_SaveH3_HierMan:
                                  Control: Button@0.0.45
                                  Properties:
                                    Height: =40
                                    OnSelect: |-
                                      =If(
                                          IsBlank(txt_H3Code_HierMan.Value) || IsBlank(txt_H3Name_HierMan.Value),
                                          Notify("Code and Name are required", NotificationType.Error),
                                          If(
                                              (sld_H3Metric1_HierMan.Value + sld_H3Metric2_HierMan.Value + sld_H3Metric3_HierMan.Value + sld_H3Metric4_HierMan.Value) <> 100,
                                              Notify("Metrics must total exactly 100%", NotificationType.Error),
                                              If(
                                                  (sld_H3Driver1_HierMan.Value + sld_H3Driver2_HierMan.Value + sld_H3Driver3_HierMan.Value + sld_H3Driver4_HierMan.Value) <> 100,
                                                  Notify("Drivers must total exactly 100%", NotificationType.Error),
                                                  If(
                                                      IsBlank(SelectedH3) || SelectedH3.Hierarchy3_ID = 0,
                                                      // INSERT
                                                      With(
                                                          {newID: Func_InsertH3(
                                                              SelectedVersion.ID,
                                                              SelectedH2.Hierarchy2_ID,
                                                              Value(txt_H3Code_HierMan.Value),
                                                              txt_H3Name_HierMan.Value,
                                                              Value(txt_H3Flow_HierMan.Value),
                                                              Value(txt_H3MinBays_HierMan.Value),
                                                              Value(txt_H3MaxBays_HierMan.Value),
                                                              Value(txt_H3DOS_HierMan.Value),
                                                              Value(txt_H3COS_HierMan.Value),
                                                              Value(txt_H3BRT_HierMan.Value),
                                                              If(tog_H3Exclude_HierMan.Checked, "Y", "N"),
                                                              txt_H3Trend_HierMan.Value,
                                                              sld_H3Metric1_HierMan.Value,
                                                              sld_H3Metric2_HierMan.Value,
                                                              sld_H3Metric3_HierMan.Value,
                                                              sld_H3Metric4_HierMan.Value,
                                                              sld_H3Driver1_HierMan.Value,
                                                              sld_H3Driver2_HierMan.Value,
                                                              sld_H3Driver3_HierMan.Value,
                                                              sld_H3Driver4_HierMan.Value
                                                          )},
